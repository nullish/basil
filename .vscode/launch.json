    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Run  multiElement from config",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "args": ["./example-config/multiElement.json"],
            "program": "${workspaceFolder}/index.js"
        },{
            "type": "node",
            "request": "launch",
            "name": "Run Get Element from config",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "args": ["placementLink"],
            "program": "${workspaceFolder}/index.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Run redirects from config",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "args": ["quietRedirect"],
            "program": "${workspaceFolder}/index.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Match link array",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "args": ["linkArray"],
            "program": "${workspaceFolder}/index.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Test List Crawl",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/scripts/listCrawl.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Test Scroll Crawl",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/scripts/scrollCrawler.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Scroll crawl",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "args": ["eg-lazyload"],
            "program": "${workspaceFolder}/index.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "checkForElement",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "args": ["count-headings"],
            "program": "${workspaceFolder}/index.js"
        }
    ]
}
